<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.web.WebView?>

<?import com.jfoenix.controls.JFXTextArea?>
<?import com.jfoenix.controls.JFXButton?>
<BorderPane fx:id="borderPane" prefWidth="1269.0"
            xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.primer.gui.uicomponents.batch.BatchHelperController"
            prefHeight="727.0"
            style="-fx-background-color: WHITE;">
    <center>
        <AnchorPane prefHeight="556.0" prefWidth="390.0" BorderPane.alignment="CENTER">
            <Label layoutX="14.0" layoutY="-2.0" prefHeight="32.0" prefWidth="75.0" text="控制台"/>
            <JFXTextArea fx:id="console" layoutX="4.0" layoutY="29.0" minHeight="-Infinity" minWidth="-Infinity"
                        style="-fx-border-color: WHITE" prefHeight="534.0" prefWidth="370.0"/>
        </AnchorPane>
    </center>
    <right>
        <AnchorPane fx:id="right" prefHeight="565.0" prefWidth="586.0" BorderPane.alignment="CENTER">
            <Label layoutY="-2.0" prefHeight="32.0" prefWidth="75.0" text="批次监控"/>
            <WebView fx:id="webView" layoutX="5.0" layoutY="28.0" prefHeight="533.0" prefWidth="571.0"/>
        </AnchorPane>
    </right>
    <bottom>
        <AnchorPane prefHeight="147.0" prefWidth="1269.0" BorderPane.alignment="CENTER">
            <JFXButton buttonType="RAISED" ripplerFill="#42bc5c"
                       style="-fx-font-size:14px;-fx-background-color:#ffffff;" fx:id="updateBathDate" layoutX="1018.0"
                       layoutY="14.0"
                       mnemonicParsing="false"
                       onAction="#updateBatchDate" prefHeight="43.0" prefWidth="114.0" text="修改批次时间"/>
            <DatePicker fx:id="batchDate" layoutX="849.0" layoutY="14.0" prefHeight="43.0" prefWidth="160.0"/>
            <ButtonBar layoutX="139.0" layoutY="55.0" prefHeight="60.0" prefWidth="1116.0">
                <buttons>
                    <JFXButton buttonType="RAISED" ripplerFill="#42bc5c"
                               style="-fx-font-size:14px;-fx-background-color:#ffffff;" fx:id="clearBtn"
                               mnemonicParsing="false" onAction="#clear"
                               prefHeight="43.0"
                               prefWidth="114.0"
                               text="清除跑批日志"/>
                    <JFXButton buttonType="RAISED" ripplerFill="#42bc5c"
                               style="-fx-font-size:14px;-fx-background-color:#ffffff;" fx:id="runBatch"
                               mnemonicParsing="false" onAction="#runBatch"
                               prefHeight="43.0"
                               prefWidth="89.0"
                               text="跑批"/>
                    <JFXButton buttonType="RAISED" ripplerFill="#42bc5c"
                               style="-fx-font-size:14px;-fx-background-color:#ffffff;" mnemonicParsing="false"
                               onAction="#openWebView" prefHeight="43.0"
                               prefWidth="114.0"
                               text="查看跑批状态"/>
                    <JFXButton buttonType="RAISED" ripplerFill="#42bc5c"
                               style="-fx-font-size:14px;-fx-background-color:#ffffff;" fx:id="extractSqlBtn"
                               mnemonicParsing="false" prefHeight="43.0"
                               prefWidth="114.0"
                               text="提取脚本"/>
                </buttons>
            </ButtonBar>
            <MenuBar layoutX="7.0" layoutY="121.0" prefHeight="25.0" prefWidth="1261.0"/>
            <JFXButton buttonType="RAISED" ripplerFill="#42bc5c"
                       style="-fx-font-size:14px;-fx-background-color:#ffffff;" layoutX="1141.0" layoutY="14.0"
                       mnemonicParsing="false"
                       onAction="#getBatchLog" prefHeight="43.0"
                       prefWidth="114.0" text="查看跑批Log"/>
        </AnchorPane>
    </bottom>
    <left>
        <AnchorPane prefHeight="569.0" prefWidth="302.0" BorderPane.alignment="CENTER">
            <Label layoutX="14.0" layoutY="-2.0" prefHeight="32.0" prefWidth="66.0" text="环境"/>
            <ListView fx:id="envList" layoutX="14.0" layoutY="30.0" prefHeight="200.0" prefWidth="277.0"/>
            <Label layoutX="14.0" layoutY="231.0" prefHeight="32.0" prefWidth="75.0" text="批次"/>
            <ListView fx:id="batchList" layoutX="14.0" layoutY="261.0" prefHeight="301.0" prefWidth="276.0"/>
        </AnchorPane>
    </left>
</BorderPane>
